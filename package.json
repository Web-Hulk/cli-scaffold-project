{
  "name": "cli-frameworks-setup",
  "version": "1.0.0",
  "author": "Web-Hulk",
  "description": "CLI to scaffold projects with different frameworks",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "cli-frameworks-setup": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.4.0",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
